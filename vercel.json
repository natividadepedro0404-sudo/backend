{
    "version": 2,
    "routes": [
        {
            "src": "/style.css",
            "dest": "/style.css"
        },
        {
            "src": "/app.js",
            "dest": "/app.js"
        },
        {
            "src": "/config.js",
            "dest": "/config.js"
        },
        {
            "src": "/(.*)",
            "dest": "/index.html"
        }
    ]
}